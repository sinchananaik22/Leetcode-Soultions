def sortStack(stack):
    aux = []
    
    while stack:
        # Pop top element from original stack
        temp = stack.pop()
        
        # Move elements from aux to stack if they are bigger than temp
        while aux and aux[-1] > temp:
            stack.append(aux.pop())
        
        aux.append(temp)
    
    # Copy back sorted elements to original stack
    while aux:
        stack.append(aux.pop())
    
    return stack
